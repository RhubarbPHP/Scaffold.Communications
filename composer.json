{
  "name": "rhubarbphp/scaffold-communications",
  "description": "Provides a basic layer for handling communications",
  "keywords": [
    "rhubarb",
    "php",
    "communications",
    "email",
    "orders"
  ],
  "homepage": "http://www.rhubarbphp.com/",
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "Rhubarb\\Scaffolds\\Communications\\": "src/",
      "Rhubarb\\Scaffolds\\Communications\\Tests\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  },
  "require": {
    "rhubarbphp/rhubarb": "*",
    "rhubarbphp/module-stem": "*",
    "rhubarbphp/module-leaf": "^1.0.0",
    "rhubarbphp/module-leaf-common-controls": "^1.0.8",
    "rhubarbphp/scaffold-background-tasks": "*",
    "rhubarbphp/scaffold-application-settings": "^1.0.0",
    "rhubarbphp/module-leaf-crud": "^1.0"
  },
  "require-dev": {
    "codeception/codeception": "^2.0.0",
    "rhubarbphp/custard": ">1.0",
    "phpmd/phpmd": "*",
    "squizlabs/php_codesniffer": "*",
    "sebastian/phpcpd": "*",
    "rhubarbphp/module-build-status-updater": "dev-debug as 1.0.3",
    "tan-tan-kanarek/github-php-client": "dev-master as 1.0"
  }
}
